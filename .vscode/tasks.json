{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "buildall",
			"type": "shell",
			"command": "cargo make build",
			"options": {
			  "cwd": "${workspaceRoot}"
			},
			"group": {
			  "kind": "build",
			  "isDefault": true
			}
        },
        {
            "label": "tests",
			"type": "shell",
			"command": "cargo make test",
			"options": {
			  "cwd": "${workspaceRoot}"
			},
			"group": {
                "kind": "test",
                "isDefault": true
              }
		},
	]
}
